{
  "name": "react-redux-universal-starter",
  "version": "1.0.0",
  "description": "Starter kit for react, redux, and server-side rendering",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node index.js",
    "build": "webpack --config webpack.prod.config.js",
    "dev:ssr": "concurrently --kill-others \"npm run watch:ssr-client\" \"npm run watch:ssr-server\"",
    "dev:client": "node dev-server.js",
    "watch:ssr-server": "nodemon index.js",
    "watch:ssr-client": "webpack --watch --config webpack.base.config.js"
  },
  "author": "Stefan Huynh",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "express": "^4.14.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "reselect": "^2.5.4",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
